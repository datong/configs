! Xft settings 
---------------------------------------------------------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

!URxvt*saveLines: 32767
URxvt*loginShell:   true
URxvt*cutchars:     "()*,<>[]{}|'
URxvt*secondaryScroll:   true
urxvt*perl-lib:     /usr/lib/urxvt/perl/
urxvt*perl-ext-common:   default,matcher
URxvt*urlLauncher:   /usr/bin/firefox
URxvt*matcher.button:   1
URxvt*matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt*visualBell:   false
URxvt*keysym.C-u: perl:matcher

Rxvt.preeditType:Root

URxvt*termName: rxvt
URxvt*background:  #000000
URxvt*foreground:  #A8A8A8
URxvt*scrollBar_right:	false
URxvt*scrollBar:	false
URxvt*borderLess:	true
URxvt*inheritPixmap:	true
URxvt*geometry:		84x24
URxvt*tintColor:	white
URxvt*shading:		35
URxvt*font: xft:terminus:size=12x24
URxvt*color0:      #000000
URxvt*color1:      #A80000
URxvt*color2:      #00A800
URxvt*color3:      #A85400
URxvt*color4:      #0000A8
URxvt*color5:      #A800A8
URxvt*color6:      #00A8A8
URxvt*color7:      #A8A8A8
URxvt*color8:      #545054
URxvt*color9:      #F85450
URxvt*color10:     #50FC50
URxvt*color11:     #F2FC50
URxvt*color12:     #5054F8
URxvt*color13:     #F854F8
URxvt*color14:     #50FCF8
URxvt*color15:     #F8FCF8
